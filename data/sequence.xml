<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="seq_patrimoine_asset_initial_code" model="ir.sequence">
        <field name="name">Code Initial Bien Patrimonial</field>
        <field name="code">patrimoine.asset.initial_code</field> 
        <field name="prefix"></field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/> 
    </record>

    <record id="seq_patrimoine_asset" model="ir.sequence">
        <field name="name">Code Matériel Patrimoine (Ancien/Autre usage)</field>
        <field name="code">patrimoine.asset.code</field>
        <field name="prefix">MAT/%(year)s/</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

    <record id="seq_patrimoine_mouvement" model="ir.sequence">
        <field name="name">Mouvement Patrimoine</field>
        <field name="code">patrimoine.mouvement.code</field>
        <field name="prefix">MVT/%(year)s/</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
    </record>
    
    <record id="seq_patrimoine_perte" model="ir.sequence">
        <field name="name">Déclaration Perte</field>
        <field name="code">patrimoine.perte.code</field>
        <field name="prefix">PERTE/%(year)s/</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

</odoo>
